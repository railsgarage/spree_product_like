<h2><%= Spree.t('spree_product_like.likes') %></h2>

<div data-hook="search_results">
  <% if @products.empty? %>
    <h6 class="search-results-title"><%= Spree.t(:no_products_found) %></h6>
<% else %>
    <%= render :partial => 'spree/shared/products', :locals => { :products => @products } %>
<% end %>
</div>
